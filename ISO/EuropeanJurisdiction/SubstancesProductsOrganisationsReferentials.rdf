	<owl:NamedIndividual rdf:about="&idmp-eura;EuropeanMedicinesAgencyRegistrationAuthority">
		<cmns-ra:manages rdf:resource="&idmp-spor;OrganisationManagementServices"/>
		<cmns-ra:manages rdf:resource="&idmp-spor;ProductManagementServices"/>
		<cmns-ra:manages rdf:resource="&idmp-spor;ReferentialsManagementServicesRegistry"/>
		<cmns-ra:manages rdf:resource="&idmp-spor;SubstancesManagementServicesRegistry"/>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&idmp-spor;OrganisationManagementServices">
		<rdf:type rdf:resource="&cmns-ra;RegistrationService"/>
		<rdfs:label>Organisation Management Services (OMS)</rdfs:label>
		<skos:definition>services supporting a central dictionary of organisation data in multiple languages</skos:definition>
		<cmns-av:abbreviation>OMS services</cmns-av:abbreviation>
		<cmns-av:directSource rdf:resource="https://spor.ema.europa.eu/"/>
		<cmns-col:isPartOf rdf:resource="&idmp-spor;SubstancesProductsOrganisationsReferentialsDataManagementServices"/>
		<cmns-prd:isProvidedBy rdf:resource="&idmp-eura;EuropeanMedicinesAgencyRegistrationAuthority"/>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&idmp-spor;ProductManagementServices">
		<rdf:type rdf:resource="&cmns-ra;RegistrationService"/>
		<rdfs:label>Product Management Services (PMS)</rdfs:label>
		<skos:definition>services supporting a central dictionary of product data in multiple languages</skos:definition>
		<cmns-av:abbreviation>PMS service</cmns-av:abbreviation>
		<cmns-av:adaptedFrom>https://spor.ema.europa.eu/sporwi/</cmns-av:adaptedFrom>
		<cmns-col:isPartOf rdf:resource="&idmp-spor;SubstancesProductsOrganisationsReferentialsDataManagementServices"/>
		<cmns-prd:isProvidedBy rdf:resource="&idmp-eura;EuropeanMedicinesAgencyRegistrationAuthority"/>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&idmp-spor;ReferentialsManagementServices">
		<rdf:type rdf:resource="&cmns-ra;RegistrationService"/>
		<rdfs:label>Referentials Management Services (RMS)</rdfs:label>
		<skos:definition>services supporting a central dictionary of referentials data in multiple languages</skos:definition>
		<cmns-av:abbreviation>RMS service</cmns-av:abbreviation>
		<cmns-av:directSource rdf:resource="https://spor.ema.europa.eu/"/>
		<cmns-col:isPartOf rdf:resource="&idmp-spor;SubstancesProductsOrganisationsReferentialsDataManagementServices"/>
		<cmns-prd:isProvidedBy rdf:resource="&idmp-eura;EuropeanMedicinesAgencyRegistrationAuthority"/>
	</owl:NamedIndividual>
	
	<owl:Class rdf:about="&idmp-spor;ReferentialsManagementServicesRegistrationScheme">
		<rdfs:subClassOf rdf:resource="&cmns-ra;RegistrationScheme"/>
		<rdfs:label>Referentials Management Services (RMS) registration scheme</rdfs:label>
		<skos:definition>scheme for organizing information in the Substances Products Organisations Referentials (SPOR) Referentials Management Services (RMS) registry and allocating identifiers to registered items</skos:definition>
	</owl:Class>
	
	<owl:NamedIndividual rdf:about="&idmp-spor;ReferentialsManagementServicesRegistry">
		<rdf:type rdf:resource="&cmns-ra;Registry"/>
		<rdfs:label>Referentials Management Services (RMS) registry</rdfs:label>
		<skos:definition>central dictionary of referentials data in multiple languages available through the Substances Products Organisations Referentials (SPOR) Referentials Management Services (RMS)</skos:definition>
		<cmns-av:abbreviation>SPOR RMS</cmns-av:abbreviation>
		<cmns-av:abbreviation>SPOR-RMS</cmns-av:abbreviation>
		<cmns-av:directSource rdf:resource="https://spor.ema.europa.eu/"/>
		<cmns-ra:isManagedBy rdf:resource="&idmp-eura;EuropeanMedicinesAgencyRegistrationAuthority"/>
	</owl:NamedIndividual>
	
	<owl:Class rdf:about="&idmp-spor;SPOR-DataClassification">
		<rdfs:subClassOf rdf:resource="&idmp-spor;SPORControlledVocabularyTerm"/>
		<rdfs:label>SPOR data classification</rdfs:label>
		<skos:definition>classifier and code element that describes the nature of any data element in terms of accessibility from a Substances Products Organisations Referentials (SPOR) perspective</skos:definition>
		<cmns-av:directSource rdf:resource="https://spor.ema.europa.eu/v1/lists/200000004983"/>
	</owl:Class>
	
	<owl:Class rdf:about="&idmp-spor;SPOR-Domain">
		<rdfs:subClassOf rdf:resource="&idmp-spor;SPORControlledVocabularyTerm"/>
		<rdfs:label>SPOR domain</rdfs:label>
		<skos:definition>classifier and code element that describes the domain in which a particular substance or product is used from a Substances Products Organisations Referentials (SPOR) perspective</skos:definition>
		<cmns-av:adaptedFrom rdf:resource="https://spor.ema.europa.eu/v1/lists/100000000004"/>
	</owl:Class>
	
	<owl:Class rdf:about="&idmp-spor;SPOR-MarketingStatusClassifier">
		<rdfs:subClassOf rdf:resource="&idmp-spor;SPORControlledVocabularyTerm"/>
		<rdfs:label>SPOR marketing status classifier</rdfs:label>
		<skos:definition>classifier and code element that describes the current status of some substance with respect to whether or not it has been placed on the market from a Substances Products Organisations Referentials (SPOR) perspective</skos:definition>
		<cmns-av:directSource rdf:resource="https://spor.ema.europa.eu/v1/lists/100000072052"/>
	</owl:Class>
	
	<owl:Class rdf:about="&idmp-spor;SPOR-ShelfLifeType">
		<rdfs:subClassOf rdf:resource="&idmp-spor;SPORControlledVocabularyTerm"/>
		<rdfs:subClassOf rdf:resource="&idmp-mprd;ShelfLifeType"/>
		<rdfs:label>SPOR shelf life type</rdfs:label>
		<skos:definition>classifier and code element that describes the point in the life cycle from manufacturing to administration to a patient that a value for shelf life applies to, including potentially with respect to a specific packaging item, from a Substances Products Organisations Referentials (SPOR) perspective</skos:definition>
		<cmns-av:directSource rdf:resource="https://spor.ema.europa.eu/v1/lists/100000073343"/>
	</owl:Class>
	
	<owl:Class rdf:about="&idmp-spor;SPOR-SpecialPrecautionsForStorage">
		<rdfs:subClassOf rdf:resource="&idmp-spor;SPORControlledVocabularyTerm"/>
		<rdfs:subClassOf rdf:resource="&idmp-mprd;SpecialPrecautionsForStorage"/>
		<rdfs:label>SPOR special precautions for storage</rdfs:label>
		<skos:definition>classifier and code element that provides precise instructions for storage of a substance or product, such as the kind of container, temperature restrictions, container management, and other restrictions on the way something must be handled from a Substances Products Organisations Referentials (SPOR) perspective</skos:definition>
		<cmns-av:directSource rdf:resource="https://spor.ema.europa.eu/v1/lists/100000073344"/>
	</owl:Class>
	
	<owl:Class rdf:about="&idmp-spor;SPOR-Status">
		<rdfs:subClassOf rdf:resource="&idmp-spor;SPORControlledVocabularyTerm"/>
		<rdfs:subClassOf rdf:resource="&mvf;TermStatus"/>
		<rdfs:label>SPOR status</rdfs:label>
		<skos:definition>classifier and code element that describes the state of something in a Substances Products Organisations Referentials (SPOR) registry as of some point in time</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&idmp-spor;SPOR-Visibility">
		<rdfs:subClassOf rdf:resource="&idmp-spor;SPORControlledVocabularyTerm"/>
		<rdfs:label>SPOR visibility</rdfs:label>
		<skos:definition>classifier and code element that characterizes the vocabulary term with respect to whether or not it is public, has restricted access, or other visibility in Substances Products Organisations Referentials (SPOR)</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&idmp-spor;SPORControlledVocabulary">
		<rdfs:subClassOf rdf:resource="&cmns-cls;ClassificationScheme"/>
		<rdfs:subClassOf rdf:resource="&cmns-cds;CodeSet"/>
		<rdfs:subClassOf rdf:resource="&mvf;Vocabulary"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-col;isIncludedIn"/>
				<owl:hasValue rdf:resource="&idmp-spor;ReferentialsManagementServicesRegistry"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&idmp-spor;hasSPORVisibility"/>
				<owl:onClass rdf:resource="&idmp-spor;SPOR-Visibility"/>
				<owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&idmp-spor;hasSPORDomain"/>
				<owl:onClass rdf:resource="&idmp-spor;SPOR-Domain"/>
				<owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&idmp-spor;hasSPORStatus"/>
				<owl:onClass rdf:resource="&idmp-spor;SPOR-Status"/>
				<owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-id;isIdentifiedBy"/>
				<owl:onClass rdf:resource="&idmp-spor;SPORListIdentifier"/>
				<owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-dsg;isSignifiedBy"/>
				<owl:onClass rdf:resource="&idmp-spor;SPORListName"/>
				<owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>SPOR controlled vocabulary</rdfs:label>
		<skos:definition>controlled vocabulary used to classify entities included in the Substances Products Organisations Referentials (SPOR) Referentials Management Services (RMS) related services and repositories</skos:definition>
		<cmns-av:synonym>SPOR List</cmns-av:synonym>
	</owl:Class>
	
	<owl:Class rdf:about="&idmp-spor;SPORControlledVocabularyTerm">
		<rdfs:subClassOf rdf:resource="&cmns-cls;Classifier"/>
		<rdfs:subClassOf rdf:resource="&cmns-cds;CodeElement"/>
		<rdfs:subClassOf rdf:resource="&mvf;VocabularyEntry"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&mvf;hasBroaderEntry"/>
				<owl:allValuesFrom rdf:resource="&idmp-spor;SPORControlledVocabularyTerm"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&mvf;hasNarrowerEntry"/>
				<owl:allValuesFrom rdf:resource="&idmp-spor;SPORControlledVocabularyTerm"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&idmp-spor;hasSPOROtherName"/>
				<owl:onClass rdf:resource="&idmp-spor;SPORTermName"/>
				<owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&idmp-spor;hasSPORShortName"/>
				<owl:onClass rdf:resource="&idmp-spor;SPORTermName"/>
				<owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-dsg;denotes"/>
				<owl:onClass>
					<owl:Class>
						<owl:unionOf rdf:parseType="Collection">
							<rdf:Description rdf:about="&idmp-sub;Substance">
							</rdf:Description>
							<rdf:Description rdf:about="&cmns-prd;Product">
							</rdf:Description>
						</owl:unionOf>
					</owl:Class>
				</owl:onClass>
				<owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-cls;classifies"/>
				<owl:onClass>
					<owl:Class>
						<owl:unionOf rdf:parseType="Collection">
							<rdf:Description rdf:about="&idmp-sub;Substance">
							</rdf:Description>
							<rdf:Description rdf:about="&cmns-prd;Product">
							</rdf:Description>
						</owl:unionOf>
					</owl:Class>
				</owl:onClass>
				<owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&idmp-spor;hasSPORStatus"/>
				<owl:onClass rdf:resource="&idmp-spor;SPOR-Status"/>
				<owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&idmp-spor;hasSPORTermName"/>
				<owl:someValuesFrom rdf:resource="&idmp-spor;SPORTermName"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-col;isIncludedIn"/>
				<owl:someValuesFrom rdf:resource="&idmp-spor;SPORControlledVocabulary"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>SPOR controlled vocabulary term</rdfs:label>
		<skos:definition>term in a controlled vocabulary whose values describe some substance or product from a Substances Products Organisations Referentials (SPOR) perspective</skos:definition>
		<cmns-av:synonym>SPOR RMS controlled term</cmns-av:synonym>
		<cmns-av:synonym>SPOR RMS term</cmns-av:synonym>
		<cmns-av:synonym>SPOR term</cmns-av:synonym>
	</owl:Class>
	
	<owl:Class rdf:about="&idmp-spor;SPORListIdentifier">
		<rdfs:subClassOf rdf:resource="&cmns-ra;RegisteredIdentifier"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-ra;isRegisteredBy"/>
				<owl:hasValue rdf:resource="&idmp-eura;EuropeanMedicinesAgencyRegistrationAuthority"/>
			</owl:Restriction>
		</rdfs:subClassOf>
